<div class="page">
    <a href="{{url}}">
        <div class="article__date">{{date}}</div>
        <h3 class="article__title h2">{{item.title}}</h3>
        <figure class="article__image"><img src="assets/img/{{item.images}}" alt="" /></figure>
        <p class="article__description">{{item.description}}</p>
    </a>
</div>
